{
  "compilerOptions": {
    "target": "es5",
    "removeComments": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "commonjs",
    "noImplicitAny": true
  },
  "exclude": ["../src/*/**"]
}
