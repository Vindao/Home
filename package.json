{
  "name": "vindao_home_quasar",
  "version": "0.0.1",
  "description": "Homepage of \"Vindao UI development\"",
  "productName": "Vindao Home",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "VindaoCore <vindao@outlook.com>",
  "private": true,
  "scripts": {
    "git:graph": "git log --all --decorate --oneline --graph",
    "client:ssr": "quasar dev -m ssr",
    "client:pwa": "quasar dev -m pwa",
    "client:spa": "quasar dev -m spa",
    "build:ssr": "quasar build -m ssr",
    "git:commit": "git add . && git commit -m",
    "api:build": "webpack --config src-ssr/config/webpack.prod.js",
    "api:start": "npm run start --prefix api",
    "api:dev": "npm run dev --prefix api",
    "server:dev": "nodemon server.js & npm run dev --prefix server",
    "dev:ssr": "npm run client:ssr & npm run api:dev",
    "dev:pwa": "npm run client:pwa & npm run api:dev",
    "dev:spa": "npm run client:spa & npm run api:dev"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.1",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "express-http-to-https": "^1.1.4",
    "gsap": "^3.1.1",
    "mongoose": "^5.9.2",
    "quasar": "^1.0.0",
    "serve-static": "^1.14.1",
    "vue-i18n": "^8.0.0",
    "vuescroll": "^4.15.0-beta.1"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "@quasar/quasar-app-extension-icon-genie": "^1.1.3",
    "@quasar/quasar-app-extension-typescript": "^1.0.0-beta.2",
    "@types/express": "^4.17.2",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^11.9.5",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "cors": "^2.8.5",
    "eslint-config-prettier": "^6.0.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.3.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
